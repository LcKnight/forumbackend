<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.forumbackend.Mapper.UpfileMapper">
    <update id="add_purchasetime">
        update upfile
        set purchase_time=purchase_time+1
        where upfile_resource_id=#{rid}
    </update>
</mapper>